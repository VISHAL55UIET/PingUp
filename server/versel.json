{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "server/configs/**",
          "server/inngest/**",
          "server/models/**",
          ".env"
        ]
      }
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/server.js" }
  ]
}
